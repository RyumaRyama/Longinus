<div class="page-header">
  <h3><%= @title %>一覧 </h3>
</div>

<div class="col-md-8">
  <% if @paginates.any? %>
    <% @paginates.each do |paginate| %>
      <%= link_to paginate.name, user_profile_path(account: paginate.account), class: 'list-group-item'  %>
    <% end %>
    <%= paginate @paginates %>
  <% end %>
</div>

<br>
<br>

<aside class="col-md-4">
  <section class="user_info">
    <span><%= link_to "view my profile", user_profile_path %></span>
  </section>
</aside>
